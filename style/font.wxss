@font-face {
  font-family: 'icomoon';
  src:  url('https://28718514.qcloud.la/fonts/icomoon.eot?izy7yi');
  src:  url('https://28718514.qcloud.la/fonts/icomoon.eot?izy7yi#iefix') format('embedded-opentype'),
    url('https://28718514.qcloud.la/fonts/icomoon.ttf?izy7yi') format('truetype'),
    url('https://28718514.qcloud.la/fonts/icomoon.woff?izy7yi') format('woff'),
    url('https://28718514.qcloud.la/fonts/icomoon.svg?izy7yi#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

[class^="icon--"], [class*=" icon--"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'icomoon' !important;
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon--plus:before {
  content: "\e900";
}
.icon--male:before {
  content: "\e901";
  color: #66b3e9;
}
.icon--female:before {
  content: "\e902";
  color: #e78278;
}
.icon--delete:before {
  content: "\e903";
}

